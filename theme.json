{
	"$schema": "https://schemas.wp.org/wp/6.9/theme.json",
	"version": 3,
	"settings": {
		"appearanceTools": true,
		"color": {
			"defaultPalette": false,
			"palette": [
				{
					"color": "#000000",
					"name": "zwart",
					"slug": "fngllw-zwart"
				},
				{
					"color": "#ffffff",
					"name": "wit",
					"slug": "fngllw-wit"
				},
				{
					"color": "#cccccc",
					"name": "text",
					"slug": "fngllw-text"
				},
				{
					"color": "#294c3f",
					"name": "background",
					"slug": "fngllw-background"
				},
				{
					"color": "#1b3229",
					"name": "background header",
					"slug": "fngllw-background-header"
				},
				{
					"color": "#f5c9b0",
					"name": "background footer",
					"slug": "fngllw-background-footer"
				},
				{
					"color": "#cfe718",
					"name": "accent citroen",
					"slug": "custom-accent-citroen"
				},
				{
					"color": "#509f2f",
					"name": "accent groen ",
					"slug": "custom-accent-groen"
				},
				{
					"color": "#427b66",
					"name": "groen 3",
					"slug": "custom-groen-3"
				},
				{
					"color": "#59a68a",
					"name": "groen 4",
					"slug": "custom-groen-4"
				},
				{
					"color": "#8bc1ad",
					"name": "groen 5",
					"slug": "custom-groen-5"
				},
				{
					"color": "#bddcd1",
					"name": "groen 6",
					"slug": "custom-groen-6"
				},
				{
					"color": "#deede8",
					"name": "groen 7",
					"slug": "custom-groen-7"
				}
			]
		},
		"layout": {
			"contentSize": "800px",
			"wideSize": "1200px"
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"defaultFontSizes": false,
			"fontFamilies": [
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Petrona",
							"fontStyle": "normal",
							"fontWeight": "100 900",
							"src": [
								"file:./assets/fonts/Petrona-VariableFont_wght.woff2"
							]
						},
						{
							"fontFamily": "Petrona",
							"fontStyle": "italic",
							"fontWeight": "100 900",
							"src": [
								"file:./assets/fonts/Petrona-Italic-VariableFont_wght.woff2"
							]
						}
					],
					"fontFamily": "Petrona, serif",
					"name": "Petrona",
					"slug": "petrona"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Bricolage",
							"fontStyle": "normal",
							"fontWeight": "200 800",
							"src": [
								"file:./assets/fonts/BricolageGrotesque.woff2"
							]
						}
					],
					"fontFamily": "Bricolage, sans-serif",
					"name": "Bricolage",
					"slug": "bricolage"
				},
				{
					"fontFamily": "Consolas, 'Courier New', Courier, monospace",
					"name": "Monospace",
					"slug": "monospace"
				}
			],
			"fontSizes": [
				{
					"fluid": {
						"max": "1rem",
						"min": "0.8rem"
					},
					"name": "Small",
					"size": "1rem",
					"slug": "small"
				},
				{
					"fluid": {
						"max": "1.3rem",
						"min": "1rem"
					},
					"name": "Normal",
					"size": "1.3rem",
					"slug": "normal"
				},
				{
					"fluid": {
						"max": "1.7rem",
						"min": "1.3rem"
					},
					"name": "Medium",
					"size": "1.7rem",
					"slug": "medium"
				},
				{
					"fluid": {
						"max": "2rem",
						"min": "1.7rem"
					},
					"name": "Large",
					"size": "2rem",
					"slug": "large"
				},
				{
					"fluid": {
						"max": "3rem",
						"min": "2rem"
					},
					"name": "X Large",
					"size": "3rem",
					"slug": "x-large"
				},
				{
					"fluid": {
						"max": "4rem",
						"min": "3rem"
					},
					"name": "XXLarge",
					"size": "4rem",
					"slug": "xx-large"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/code": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--monospace)"
				}
			},
			"core/heading": {
				"color": {
					"text": "var(--wp--preset--color--custom-citroen)"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--custom-citroen)"
						}
					}
				},
				"spacing": {
					"margin": {
						"bottom": "0rem",
						"top": "var(--wp--preset--spacing--40)"
					}
				}
			},
			"core/paragraph": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--petrona)",
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"core/preformatted": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--monospace)"
				}
			},
			"core/site-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-large)",
					"fontStyle": "normal",
					"fontWeight": "300"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--fngllw-background)",
			"text": "var(--wp--preset--color--fngllw-text)"
		},
		"elements": {
			"link": {
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--custom-citroen)"
					}
				},
				"color": {
					"text": "var(--wp--preset--color--fngllw-donkerrood)"
				}
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--bricolage)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"lineHeight": "1.5"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	]
}